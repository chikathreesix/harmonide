<!DOCTYPE>
<html>
<head>
  <link rel="stylesheet" href="assets/harmonide.css">
  <link rel="stylesheet" href="assets/exe.css">
  <link href='http://fonts.googleapis.com/css?family=Oswald' rel='stylesheet' type='text/css'>
  <link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/highlight.js/8.2/styles/default.min.css">
  <script src="http://cdnjs.cloudflare.com/ajax/libs/highlight.js/8.2/highlight.min.js"></script>
</head>
<body>
  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
  
    ga('create', 'UA-58040987-1', 'auto');
    ga('send', 'pageview');
  
  </script>

  <div id="presentation" class="slide_container">
    <% slides.forEach(function(slide){ %>
      <section class="<%= slide.className %>" style="<%= slide.style %>">
        <div class="slide">
          <div class="slide_content">
            <%- slide.content %>
          </div>
        </div>
      </section>
    <% }); %>
  </div>
  <aside class="controls">
    <span class="arrow right"></span>
    <span class="arrow left"></span>
  </aside>
  <script src="assets/traceur.js"></script>
  <script src="assets/harmonide.js"></script>
  <script src="assets/exe.js"></script>
  <script>
    exe.init(function(){
      var codeElems = document.querySelectorAll('pre code');
      for(var i = 0, len = codeElems.length; i < len; i++){
        hljs.highlightBlock(codeElems[i]);
      }
    });
  </script>
</body>
</html>
